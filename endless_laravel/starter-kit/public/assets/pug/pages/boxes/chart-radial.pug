- var chat_menu             = true;
- var theme_customizer      = true;
- var typeahead             = true;
- var typeahead_search      = true;
- var tooltip               = true;
- var radial_chart_tooltip  = true;

doctype html
html(lang='en')
    include ../../components/header-files
    body
        include ../../components/loader
        // page-wrapper Start
        .page-wrapper.box-layout
            include ../../components/header
            // Page Body Start
            .page-body-wrapper
                include ../../components/sidebar
                include ../../components/chat-sidebar
                .page-body
                    .container-fluid
                        .page-header
                            .row
                                .col
                                  .page-header-left
                                    h3 Radial Chart
                                    ol.breadcrumb
                                        li.breadcrumb-item
                                          a(href='index.html')
                                            i(data-feather='home')
                                        li.breadcrumb-item Charts
                                        li.breadcrumb-item.active Radial Chart
                                include ../../components/bookmark
                    // Container-fluid starts
                    .container-fluid
                      .row
                        .col-sm-12.col-xl-12
                          .card
                            .card-header
                              h5 Radial Variants
                            .card-body.redial-chart-block
                              .radial-bar.radial-bar-10.radial-bar-lg.radial-bar-primary.mr-3(data-label='10%')
                              .radial-bar.radial-bar-30.radial-bar-lg.radial-bar-secondary.mr-3(data-label='30%')
                              .radial-bar.radial-bar-50.radial-bar-lg.radial-bar-success.mr-3(data-label='50%')
                              .radial-bar.radial-bar-70.radial-bar-lg.mr-3(data-label='70%', class='.radial-bar-info')
                              .radial-bar.radial-bar-90.radial-bar-lg.radial-bar-warning.mr-3(data-label='90%')
                              .radial-bar.radial-bar-100.radial-bar-lg.radial-bar-danger.mr-3(data-label='100%')
                        .col-sm-12.col-xl-6
                          .card
                            .card-header
                              h5 Radial Sizes
                            .card-body.redial-chart-block
                              .radial-bar.radial-bar-50.radial-bar-lg.radial-bar-primary.mr-3(data-label='50%')
                              .radial-bar.radial-bar-40.radial-bar-secondary.mr-3(data-label='40%')
                              .radial-bar.radial-bar-40.radial-bar-sm.radial-bar-success.mr-3(data-label='40%')
                              .radial-bar.radial-bar-30.radial-bar-xs.radial-bar-info.mr-3(data-label='30%')
                        .col-sm-12.col-xl-6
                          .card
                            .card-header
                              h5 Radial With Images
                            .card-body.redial-chart-block
                              .radial-bar.radial-bar-50.radial-bar-lg.radial-bar-primary.mr-3(data-label='50%')
                                img(src='../assets/images/user/1.jpg', alt='User-Image')
                              .radial-bar.radial-bar-50.radial-bar-secondary.mr-3(data-label='50%')
                                img(src='../assets/images/user/1.jpg', alt='User-Image')
                              .radial-bar.radial-bar-40.radial-bar-sm.radial-bar-success.mr-3(data-label='40%')
                                img(src='../assets/images/user/1.jpg', alt='User-Image')
                              .radial-bar.radial-bar-30.radial-bar-xs.radial-bar-info.mr-3(data-label='30%')
                                img(src='../assets/images/user/1.jpg', alt='User-Image')
                        .col-sm-12.col-xl-12
                          .card
                            .card-header
                              h5 Radial With Toopltip
                            .card-body.redial-chart-block
                              .radial-bar.radial-bar-10.radial-bar-lg.radial-bar-primary.mr-3(data-label='10%', data-toggle='tooltip', data-title='10%')
                              .radial-bar.radial-bar-30.radial-bar-lg.radial-bar-secondary.mr-3(data-label='30%', data-toggle='tooltip', data-title='30%')
                              .radial-bar.radial-bar-50.radial-bar-lg.radial-bar-success.mr-3(data-label='50%', data-toggle='tooltip', data-title='50%')
                              .radial-bar.radial-bar-70.radial-bar-lg.radial-bar-info.mr-3(data-label='70%', data-toggle='tooltip', data-title='70%')
                              .radial-bar.radial-bar-90.radial-bar-lg.radial-bar-warning.mr-3(data-label='90%', data-toggle='tooltip', data-title='90%')
                              .radial-bar.radial-bar-100.radial-bar-lg.radial-bar-danger.mr-3(data-label='100%', data-toggle='tooltip', data-title='100%')
                    // Container-fluid Ends
                include ../../components/footer
        include ../../components/footer-files
